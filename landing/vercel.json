{
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1"
    }
  },
  "rewrites": [
    {
      "source": "/:match*",
      "destination": "https://dashboard-opal-two.vercel.app/:match*"
    },
    {
      "source": "/app/",
      "destination": "https://dashboard-opal-two.vercel.app"
    },
    {
      "source": "/app/:match*",
      "destination": "https://dashboard-opal-two.vercel.app/:match*"
    }
  ]
}
